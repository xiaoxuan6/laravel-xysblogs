{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "encore/laravel-admin": "^1.6",
        "fideloper/proxy": "^4.0",
        "freyo/flysystem-qcloud-cos-v5": "^1.1",
        "fukuball/jieba-php": "^0.33.0",
        "guzzlehttp/guzzle": "^6.3",
        "james.xue/laravel-admin-breadcrumb": "1.1.2",
        "james.xue/laravel-admin-grid": "^1.1",
        "james.xue/laravel-admin-sortable": "^1.0",
        "james.xue/laravel-eloquent-filter": "^3.0",
        "knplabs/packagist-api": "^1.5",
        "laravel-admin-ext/grid-lightbox": "^1.0",
        "laravel/framework": "5.7.*",
        "laravel/horizon": "2.0",
        "laravel/tinker": "^1.0",
        "laravolt/avatar": "^2.2",
        "maatwebsite/excel": "3.*",
        "mews/captcha": "^2.2",
        "overtrue/laravel-pinyin": "^4.0",
        "overtrue/laravel-socialite": "~2.0",
        "predis/predis": "^1.1",
        "qcloudsms/qcloudsms_php": "0.1.*",
        "vanry/laravel-scout-tntsearch": "^2.4",
        "yansongda/laravel-pay": "^2.0"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "friendsofphp/php-cs-fixer": "^3.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            "libraries"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "fix-style": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.php --using-cache=no --ansi"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "ext-pcntl": "7.2",
            "ext-posix": "7.2"
        }
    },
    "scripts-descriptions": {
        "fix-style": "Run style checks and fix violations."
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
